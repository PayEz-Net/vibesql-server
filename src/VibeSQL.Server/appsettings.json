{
  "VibeSQL": {
    "UseKeyVault": false,
    "HmacSecretName": "VibeHmacSecret",
    "HmacSecret": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Graylog": {
      "HostnameOrAddress": "localhost",
      "Port": "12201"
    }
  },
  "ConnectionStrings": {
    "VibeDb": "Server=localhost;Port=5432;Database=vibesql;User Id=vibesql_user;Password=CHANGE_ME;SSLMode=Disable;Pooling=true;"
  },
  "VibeQueryTimeouts": {
    "FreeSeconds": 2,
    "StarterSeconds": 5,
    "ProSeconds": 10,
    "EnterpriseSeconds": 30,
    "DefaultSeconds": 5
  },
  "VibeQueryLimits": {
    "MaxResultRows": 1000,
    "MaxQuerySizeBytes": 10240
  },
  "VibeRateLimits": {
    "Free": {
      "QueriesPerMinute": 60,
      "QueriesPerHour": 1000,
      "MaxConcurrentQueries": 2
    },
    "Starter": {
      "QueriesPerMinute": 300,
      "QueriesPerHour": 10000,
      "MaxConcurrentQueries": 5
    },
    "Pro": {
      "QueriesPerMinute": 1000,
      "QueriesPerHour": 100000,
      "MaxConcurrentQueries": 20
    },
    "Enterprise": {
      "QueriesPerMinute": 5000,
      "QueriesPerHour": 500000,
      "MaxConcurrentQueries": 50
    }
  },
  "AllowedHosts": "*"
}
